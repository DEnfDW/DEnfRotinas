CREATE TABLE RAIS_2019_CENTRO_OESTE AS SELECT * FROM RAIS_2010_GO WHERE 1=0;

## Para o ano de 2014
CREATE TABLE rais_2014_ac (
    Bairros_SP VARCHAR(255),
    Bairros_Fortaleza VARCHAR(255),
    Bairros_RJ VARCHAR(255),
    Causa_Afastamento_1 VARCHAR(255),
    Causa_Afastamento_2 VARCHAR(255),
    Causa_Afastamento_3 VARCHAR(255),
    Motivo_Desligamento VARCHAR(255),
    CBO_Ocupacao_2002 VARCHAR(255),
    CNAE_2_0_Classe VARCHAR(255),
    CNAE_95_Classe VARCHAR(255),
    Distritos_SP VARCHAR(255),
    Vinculo_Ativo_31_12 VARCHAR(255),
    Faixa_Etaria VARCHAR(255),
    Faixa_Hora_Contrat VARCHAR(255),
    Faixa_Remun_Dezem_SM VARCHAR(255),
    Faixa_Remun_Media_SM VARCHAR(255),
    Faixa_Tempo_Emprego VARCHAR(255),
    Escolaridade_apos_2005 VARCHAR(255),
    Qtd_Hora_Contr VARCHAR(255),
    Idade VARCHAR(255),
    Ind_CEI_Vinculado VARCHAR(255),
    Ind_Simples VARCHAR(255),
    Mes_Admissao VARCHAR(255),
    Mes_Desligamento VARCHAR(255),
    Mun_Trab VARCHAR(255),
    Municipio VARCHAR(255),
    Nacionalidade VARCHAR(255),
    Natureza_Juridica VARCHAR(255),
    Ind_Portador_Defic VARCHAR(255),
    Qtd_Dias_Afastamento VARCHAR(255),
    Raca_Cor VARCHAR(255),
    Regioes_Adm_DF VARCHAR(255),
    Vl_Remun_Dezembro_Nom VARCHAR(255),
    Vl_Remun_Dezembro_SM VARCHAR(255),
    Vl_Remun_Media_Nom VARCHAR(255),
    Vl_Remun_Media_SM VARCHAR(255),
    CNAE_2_0_Subclasse VARCHAR(255),
    Sexo_Trabalhador VARCHAR(255),
    Tamanho_Estabelecimento VARCHAR(255),
    Tempo_Emprego VARCHAR(255),
    Tipo_Admissao VARCHAR(255),
    Tipo_Estab1 VARCHAR(255),
    Tipo_Estab2 VARCHAR(255), -- Ajustado para evitar duplicidade
    Tipo_Defic VARCHAR(255),
    Tipo_Vinculo VARCHAR(255)
);


#comando para criar as tabelas para cada estado
CREATE TABLE RAIS_2021_AC (
    bairros_sp VARCHAR(256),
    bairros_fortaleza VARCHAR(256),
    bairros_rj VARCHAR(256),
    causa_afastamento_1 VARCHAR(256),
    causa_afastamento_2 VARCHAR(256),
    causa_afastamento_3 VARCHAR(256),
    motivo_desligamento VARCHAR(256),
    cbo_ocupacao_2002 VARCHAR(256),
    cnae_2_0_classe VARCHAR(256),
    cnae_95_classe VARCHAR(256),
    distritos_sp VARCHAR(256),
    vinculo_ativo_31_12 VARCHAR(256),
    faixa_etaria VARCHAR(256),
    faixa_hora_contrat VARCHAR(256),
    faixa_remun_dezem_sm DECIMAL(10,2),
    faixa_remun_media_sm DECIMAL(10,2),
    faixa_tempo_emprego VARCHAR(256),
    escolaridade_apos_2005 VARCHAR(256),
    qtd_hora_contr INTEGER,
    idade INTEGER,
    ind_cei_vinculado VARCHAR(256),
    ind_simples VARCHAR(256),
    mes_admissao INTEGER,
    mes_desligamento INTEGER,
    mun_trab VARCHAR(256),
    municipio VARCHAR(256),
    nacionalidade VARCHAR(256),
    natureza_juridica VARCHAR(256),
    ind_portador_defic VARCHAR(256),
    qtd_dias_afastamento INTEGER,
    raca_cor VARCHAR(256),
    regioes_adm_df VARCHAR(256),
    vl_remun_dezembro_nom DECIMAL(10,2),
    vl_remun_dezembro_sm DECIMAL(10,2),
    vl_remun_media_nom DECIMAL(10,2),
    vl_remun_media_sm DECIMAL(10,2),
    cnae_2_0_subclasse VARCHAR(256),
    sexo_trabalhador VARCHAR(256),
    tamanho_estabelecimento INTEGER,
    tempo_emprego DECIMAL(10,2),
    tipo_admissao VARCHAR(256),
    tipo_estab1 VARCHAR(256),
    tipo_estab2 VARCHAR(256),
    tipo_defic VARCHAR(256),
    tipo_vinculo VARCHAR(256)
);

